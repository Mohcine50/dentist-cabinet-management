type Query {
    """ AUTH """
    login (username: String!, password: String!): LoginResponse,
    """ ROLES """
    loadAllRoles: [Role]!

    """TREATMENTS"""
    loadAllTreatments:[Treatment]
    getTreatmentById(id: String!): Treatment
}

type Mutation {

    """STAFF"""
    addStaffMember(staff: StaffInput!): User

    """TREATMENTS"""
    deleteTreatment(id: String!): String!
    updateTreatment(id: String!): Treatment
    addTreatment(treatmentData: TreatmentInput!): Treatment



}


